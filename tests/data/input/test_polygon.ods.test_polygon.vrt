<?xml version="1.0" encoding="UTF-8"?>
<OGRVRTDataSource>
    <OGRVRTLayer name="test_polygon-test_polygon">
        <SrcDataSource relativeToVRT="1">test_polygon.ods</SrcDataSource>
        <!--Header=False-->
        <SrcLayer>test_polygon</SrcLayer>
        <Field name="id" src="id" type="Integer"/>
